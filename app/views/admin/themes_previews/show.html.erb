<div class="iphone-container">
  <% @themes.each do |theme| %>
    <div class="cover-wrapper">
      <%= image_tag theme.theme_cover.picture.url(:large) %>
      <div class="titles">
        <p class="title"><%= theme.title_for_display %></p>
        <p class="subtitle"><%= theme.subtitle_for_display %></p>
      </div>
    </div>
  <% end %>  
</div>
