<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
<meta name="description" content="<%= t('flink.description') %>">
<meta name="og:description" content="<%= t('flink.description') %>">
<meta name="google-site-verification" content="ksV4TOKBXP-aC68eKvJATSA9gTvXbz-NNvZwV56IoiY">
<meta name="apple-itunes-app" content="app-id=<%= Rails.configuration.appstore_app_id %>">
<meta name="robots" content="follow,index">
<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">

<% if !controller_name.eql? 'looks' %>
<meta property="twitter:card" content="app">
<meta property="twitter:app:id:iphone" content="<%= Rails.configuration.appstore_app_id %>">
<meta property="twitter:app:url:iphone" content="flink">
<meta property="twitter:domain" content="flink.io">
<meta property="twitter:creator" content="@flinkHQ">
<meta property="twitter:title" content="Flink">
<meta property="twitter:image" content="<%= root_url[0...-1] + image_path('flink/desktop/logo_small.png') %>">
<meta property="twitter:description" content="<%= t('flink.description') %>">
<% end %>

<meta name="fb:app_id" content="<%= Rails.configuration.facebook_app_id %>">
