{
  "name": "Ariane",
  "version": "0.6.7",
  "manifest_version": 2,
  "description": "Outil de mapping de page produit pour Viking/Saturn.",
  "icons": {
    "48": "img/rocket48.png",
    "128": "img/rocket128.png"
  },
  "browser_action": {
    "default_title": "Active Ariane",
    "default_icon": "img/rocket48.png"
  },
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "persistent": false
  },
  "commands": {
    "next-step": {
      "suggested_key": {
        "default": "Alt+N"
      },
      "description": "Go to next step"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "build/contentscript.js"
      ]
    },
    {
      "matches": [
        "https://*.shopelia.fr/admin/viking",
        "https://*.shopelia.com/admin/viking"
      ],
      "js": [
        "src/loader_cs.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "views/toolbar.html",
    "views/panel.html",
    "assets/*"
  ],
  "permissions": [
    "tabs",
    "storage",
    "*://*/*"
  ]
}