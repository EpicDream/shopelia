{
  "name": "Saturn",
  "version": "2.2.4",
  "manifest_version": 2,
  "description": "Extrateur de donn√©e produit.",
  "icons": {
    "48": "img/saturn48.png",
    "128": "img/saturn128.png"
  },
  "browser_action": {
    "default_title": "Active Saturn",
    "default_icon": "img/saturn48.png"
  },
  "background": {
    "scripts": [
      "dist/background.min.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "dist/contentscript.min.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "cookies",
    "*://*/*"
  ]
}