{
  "name": "Kanaveral",
  "version": "0.12.0",
  "manifest_version": 2,

  "description": "Outil d'aide la commande pour Shopelia.",
  "icons": {
    "48": "img/kanaveral-48.jpg",
    "128": "img/kanaveral-128.png"},
  // "default_locale": "fr",

  "browser_action": {
    "default_title": "Active Kanaveral",
    "default_icon": "img/kanaveral-48.jpg"
  },

  "background": {
    "page": "views/background.html"
  },

  "commands": {
    "unselect-button": {
      "suggested_key": {"default": "Ctrl+B"},
      "description": "Unselect current selected button"
    },
    "next-step": {
      "suggested_key": {"default": "Ctrl+N"},
      "description": "Go to next step"
    }
  },

  "web_accessible_resources": [
    "lib/*",
    "controllers/*",
    "assets/smoothness/*",
    "views/toolbar.html"
  ],
  "permissions": ["tabs", "*://*/*"]
}
